<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1" />
  <meta name="keywords" content="<%= config.keywords %>" />
  <meta name="author" content="<%= config.author %>" />
  <meta name="description" content="<%= config.description %>" />
  <% if(is_home() && page.current > 1){ %>
    <meta name="robots" content="noindex" />
  <% } %>
  <%
    var title = page.title;

    if (is_archive()){
      title = 'archive_a';

      if (is_month()){
        title += ': ' + page.year + '/' + page.month;
      } else if (is_year()){
        title += ': ' + page.year;
      }
    } else if (is_category() || title === "categories"){
      title = "category"
    } else if (is_tag() || title === "tags"){
      title = "tag"
    } else if (title === "friends") {
      title = "friend"
    } else if (title === "about") {
      title = "about"
    } 
  %>
  <title>
    <% if (title) { %>
      <%= __(title) %> 
      <% if (page.tag) { %> : <%= page.tag %><% } %>
      <% if (page.category) { %> : <%= page.category %><% } %>
      |
    <% } %>
     <%= config.title %>
  </title>

  <% if (theme.favicon.enable){ %>
    <link rel="apple-touch-icon" href="<%- theme.favicon.touch_icon %>">
    <link rel="icon" href="<%- theme.favicon.icon %>">
  <% } %>

  <!-- jquery3.3.1 -->
  <script async src="https://cdn.jsdelivr.net/gh/wellmoonloft/peach@v0.0.2/source/js/jquery.min-3.3.1.js"></script>

  <!-- hexo site css -->
  <link href="https://cdn.jsdelivr.net/gh/wellmoonloft/peach@v0.0.2/source/css/base.css" rel="stylesheet">
  <link href="https://cdn.jsdelivr.net/gh/wellmoonloft/peach@v0.0.2/source/iconfont/iconfont.css" rel="stylesheet">
  <link href="https://cdn.jsdelivr.net/gh/wellmoonloft/peach@v0.0.2/source/css/github-markdown.css" rel="stylesheet">
  <link href="https://cdn.jsdelivr.net/gh/wellmoonloft/peach@v0.0.2/source/css/highlight.css" rel="stylesheet">  

  <!-- fancybox -->
  <link href="https://cdn.jsdelivr.net/gh/wellmoonloft/peach@v0.0.2/source/css/jquery.fancybox.min-3.5.2.css" rel="stylesheet">
  <script async src="https://cdn.jsdelivr.net/gh/wellmoonloft/peach@v0.0.2/source/js/jquery.fancybox.min-3.5.2.js"></script>
  <script async src="https://cdn.jsdelivr.net/gh/wellmoonloft/peach@v0.0.2/source/js/fancybox.js"></script>

  <% if(theme.gtag.enable) { %>
    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=<%- theme.gtag.gtagkey %>"></script>
    <script>
      window.dataLayer = window.dataLayer || [];
      function gtag(){dataLayer.push(arguments);}
      gtag('js', new Date());

      gtag('config', '<%- theme.gtag.gtagkey %>');
    </script>
  <% } %>

</head>
